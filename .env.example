
# Example environment variables for Sting trading platform

# Binance Gateway Configuration
# Get testnet credentials from https://testnet.binance.vision/
BINANCE_TESTNET_SPOT_API_KEY=${BINANCE_TESTNET_SPOT_API_KEY}
BINANCE_TESTNET_SPOT_SECRET_KEY=${BINANCE_TESTNET_SPOT_SECRET_KEY}
BINANCE_TESTNET_FUTURES_API_KEY=${BINANCE_TESTNET_FUTURES_API_KEY}
BINANCE_TESTNET_FUTURES_SECRET_KEY=${BINANCE_TESTNET_FUTURES_SECRET_KEY}

# Production Binance credentials (use with extreme caution)
BINANCE_API_KEY=your_production_api_key_here
BINANCE_SECRET=your_production_secret_here

# Other Broker API Keys (replace with your actual keys)
IB_API_KEY=your_ib_api_key
IB_SECRET_KEY=your_ib_secret_key

# Environment Configuration
# Options: production, qa, development, testing
TRADING_ENV=testing

# Database Configuration
DATABASE_URL=postgresql://user:password@localhost:5432/sting_db

# EventEngine Configuration
EVENT_ENGINE_LOG_LEVEL=INFO
EVENT_ENGINE_QUEUE_SIZE=10000

# Gateway Configuration
GATEWAY_TIMEOUT_CONNECT=30.0
GATEWAY_TIMEOUT_REQUEST=10.0
GATEWAY_ENABLE_DEBUG=true

# Testing Configuration
PYTEST_SKIP_INTEGRATION=false
TEST_SYMBOL=BTCUSDT
TEST_QUANTITY=0.01

# Other settings
LOG_LEVEL=INFO
ENVIRONMENT=development  # or production
